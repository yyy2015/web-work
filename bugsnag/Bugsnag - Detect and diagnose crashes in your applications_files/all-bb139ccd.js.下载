$(function(){$("[data-js-pricing-type]").on("click",function(){var t;return t=$(this),$(".PlanSelector-type.is-active, .PlanSelector-details.is-active").removeClass("is-active"),t.addClass("is-active"),$("#"+t.attr("data-js-pricing-type")).addClass("is-active")});var t=$("form[data-contact-form]"),a=t.find("[type=submit]"),e=t.find(".PlanSelector-contact-success");return t.submit(function(i){return a.attr("disabled","disabled"),$.ajax({url:t.attr("action"),type:"POST",data:t.serialize()}).then(function(){return a.hide(),e.show()})})}),$("[data-show-intercom]").on("click",function(t){window.Intercom&&(Intercom("showNewMessage"),t.preventDefault())}),window.Bugsnag&&(window.Bugsnag.autoNotify="bugsnag.com"===window.location.hostname);